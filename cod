peso = float (input ('Digite o seu peso (KG):'))  
altura = float (input ('Digite sua altura ):'))
IMC = peso / (altura*altura)
print('Seu IMC Ã© {:.1f} .'.format(IMC))

if IMC < 18.5:
  print('Seu indice esta baxio, voce esta mais magro do que o recomendado')

elif IMC <= 25 and IMC >= 18.6: 
  print('Seu peso esta dentro do recomendado')

elif IMC >= 25.1:
  print ('Seu indice esta maior que o recomendado, voce esta com sobrepeso')
